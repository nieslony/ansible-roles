[Autoconfig]
Revision=1

[Data Source]
DisplayName={{ organization }}/{{ item.name }} (${email})
Enabled=true
Parent=carddav-stub

[Authentication]
Host={{ evolution_dav_server }}
Method=GSSAPI
Port=443
ProxyUid=system-proxy
RememberPassword=true
User=${USER}
CredentialName=
IsExternal=false

[Contacts Backend]
IncludeMe=true

[WebDAV Backend]
AvoidIfmatch=false
CalendarAutoSchedule=false
Color=
DisplayName=Contacts
EmailAddress=
ResourcePath=/nextcloud/remote.php/dav/addressbooks/users/${USER}/{{ item.folder }}/
ResourceQuery=
SslTrust=
Order=4294967295

[Security]
Method=tls

[Refresh]
Enabled=true
IntervalMinutes=5

[Address Book]
BackendName=carddav
Order=4294967295

[Autocomplete]
IncludeMe=true
